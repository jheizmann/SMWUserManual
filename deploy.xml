<?xml version="1.0" encoding="ISO-8859-1"?>
<deploydescriptor>
	<global>
		<id>smwusermanual</id>
		<version>1.0.5</version>
		<instdir>extensions/SMWUserManual</instdir>
		<vendor>ontoprise GmbH</vendor>
		<description>This content bundle contains all help articles for the context sensitive help. It also contains some properties to determine which help article to display. These articles are necessary for the User Manual extension.</description>
		<maintainer>ontoprise GmbH</maintainer>
		<helpurl>http://smwforum.ontoprise.com/smwforum/index.php/Help:User_Manual_extension </helpurl>
		<license>GPL_v2</license>
		<dependencies>
			<dependency from="1.6.0">smwhalo</dependency>
			<dependency from="1.5.6">smwplus</dependency>
		</dependencies>
		<notice/>
	</global>
	
	<wikidumps>
		<file loc="import/smwusermanual.xml"/>
	</wikidumps>
	
	<resources>
		<!-- empty -->
	</resources>

    <configs>
        <new>
            <require name="smwusermanual"
                file="extensions/SMWUserManual/includes/SMW_UserManual.php" />
            <function name="enableSMWUserManual">
            </function>
        </new>
    </configs>
</deploydescriptor>
