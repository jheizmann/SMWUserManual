<?xml version="1.0" encoding="ISO-8859-1"?>
<deploydescriptor>
	<global>
		<id>smwusermanual</id>
		<title>User Manual extension</title>
		<version>1.0.5</version>
		<instdir>extensions/SMWUserManual</instdir>
		<vendor>ontoprise GmbH</vendor>
		<description>
			The User Manual extension provides direct access to help and context-sensitive selection of help pages. Users can submit direct feedback (such as bug issues) and ask questions in the online community.
		</description>
		<maintainer>ontoprise GmbH</maintainer>
		<helpurl>http://smwforum.ontoprise.com/smwforum/index.php/Help:User_Manual_extension </helpurl>
		<license>GPL_v2</license>
		<dependencies>
			<dependency from="1.6.0" to="1.6.0">smwhalo</dependency>
			<dependency from="1.5.6">smwplus</dependency>
		</dependencies>
		<notice/>
	</global>
	
	<wikidumps>
		<file loc="import/smwusermanual.xml"/>
	</wikidumps>
	
	<resources>
		<!-- empty -->
	</resources>

    <configs>
        <new>
            <require name="smwusermanual"
                file="extensions/SMWUserManual/includes/SMW_UserManual.php" />
            <function name="enableSMWUserManual">
            </function>
        </new>
    </configs>
</deploydescriptor>
